
class Test {
	public static void main(String[] args){
		int num0 = 1 , num1 = 1, num2 = 1;
		while(num2 < 10){
			num0 = 1;
			num1 = num2;
			num2 = num0 + num1;
			System.out.println(num0);
			System.out.println(num1);
			System.out.println(num2);
			System.out.println("-");
		}
		System.out.println(num0);
		System.out.println(num1);
		System.out.println(num2);
	}
}